( data_0375 "Ba{mxilongile} a{bahlengi} kanye no{mntwana} wakhe, base be{nquma} ukuthi ba{lethwe lapha} bazo{bonelwa} e{duzane} o{dokotela}." )
( data_0376 "Amanye a{mazwe} anezi{nhlelo} ezi{thile} zokwe{thula} u{xhaso} lwe{mishini} e{thile} e{mabhizinisi}ni." )
( data_0377 "E{thula} no{sizo} lwezi{mali} olu{qondene} na{makhono} oku{thenga}." )
( data_0378 "U{mshini} o{lindelwe} {mayelana} noku{gcina} i{banga} loku{phakama} no{hlelo}, nokuthi lu{fakiwe} futhi lu{sebenza} nge{ndlela} e{qondile} u{hlelo} loku{qwashisa}." )
( data_0379 "K{anjalo} awu{kho} u{mgomo} noma i{zihibe} zoku{nqanda} u{msebenzi}." )
( data_0380 "I{nhlangano} ye{zilimi} i{sebenza} njenge{ngxenye} ka{Hulumeni} yongoti be{zilimi}, ngoku{humusha} i{mibhalo} e{bhalwe} ngo{lunye} u{limi} olw{amukelekile}." )
( data_0381 "Iyi{humushela} kwe{zinye} i{zilimi} eza{mukelekile} ngoko{mthetho}." )
( data_0382 "Ekw{enzeni} u{hlelo} lwakho lo{nyaka}, unga{bhekelela} lokhu oku{landela}yo." )
( data_0383 "Izin{suku} lapho u{masipala} ezobe e{bambe} ngazo izi{ngxoxo} zoku{bonisa}na onga{thanda} uku{bamba} i{qhaza} kuzo." )
( data_0384 "Njengo{hlelo} loku{thuthukisa} oku{hlanganisa}yo, kanye noku{hanjiswa} kwezi{mali}, izi{ndaba} zo{mphakathi} ezi{semqoka}." )
( data_0385 "Yini okung{enziwa} yi{komiti} le{wadi} uku{siza}?" )
( data_0386 "Ku{mele} ukw{enze} nini lokhu?" )
( data_0387 "I{phoyisa} ku{mele} li{mazise} futhi li{mchazele} wonke la {malungelo} lowo {muntu} o{boshwayo}." )
( data_0388 "{Malenze} ne{siqinisekiso} sokuthi uyawa{qondisisa} ngokwe{sigaba} sa{mashumi} a{mathathu} na{nhlanu} so{Mthethosisekelo}." )
( data_0389 "Lapho kungu{nobhala} wo{mholi} we{ndabuko} noma omunye u{mholi} wo{mphakathi} o{gunyazwe} u{mholi} we{ndabuko}, kuya{dingeka} ukuthi ku{be} ne{ncwadi} yoku{ncoma} e{bhalwe} ngu{mholi} we{ndabuko}, ukuthi i{phelezele} i{sicelo}." )
( data_0390 "U{muntu} o{cela}yo ku{fanele} a{sho} ukuthi u{funa} i{khophi} ye{rekhodi} yini noma u{funa} uku{za} azoli{bheka} e{hhovisi} le{sikhungo} so{mphakathi}." )
( data_0391 "{Geza} i{zandla} zakho e{manzi}ni a{bilayo} noma e{lashiwe}, {emva} koku{sebenzisa indlu} ya{ngasese} nanga{phambi} koku{thinta} u{kudla}." )
( data_0392 "U{ngqongqoshe} enga{qokela} uku{sebenza} kwalo {Mthetho}, noma i{zigaba} ezi{thile} zawo kuyo yonke i{Ningizimu} {Afrika}." )
( data_0393 "Noma ezi{ndaweni} ezi{thile} ku{phela}, ukuba zi{qondane} no{muntu} o{thile}." )
( data_0394 "Lowo {muntu} a{be} on{ogqozi} noma ono{lwazi} olu{thile} nge{nyama} noma ngoku{phepha} kwaleyo {nyama}." )
( data_0395 "U{Senzo} u{vela} e{sigcawini} soku{qala} nje e{nkundleni} yoku{qala} a{bonakale} e{thanda} u{Nozi} futhi e{funa} uku{shada} naye." )
( data_0396 "Ngoku{cacile}, i{nkosikazi} e{shade} ngo{siko} lwase{ntshona} ne{zingane} zi{lahlekelwa} yi{sikhundla} esi{ngcono}." )
( data_0397 "Za{kleliswa} ngoku{lingana}yo no{nkosikazi} ne{zingane} ezi{lahlwe} ngenxa yo{mshado} we{sintu}." )
( data_0398 "Cha ne{mbala} i{gane} e{sokeni} {mfo} ka{Labase}, nami naka{njani} ngizoku{vakashela} kule {mpelasonto} ngizo{bona} lo{phuma langa} si{kothe} we{ntombi}." )
( data_0399 "{Khuluma} no{muntu} ose{ndaweni} e{yodwa} naye, ku{be} khona inani elinganga{magxathu} e{zinyawo} ezi{ntathu} noma ezi{nhlanu} uma u{khuluma}." )
( data_0400 "{Hlamba} i{zandla} nge{muva} koku{qala} uku{lungisela} uku{pheka} u{kudla}." )
( data_0401 "Nanga{phambi} koku{phazamiseka}, okunga{ba} uku{shintsha} noma uku{hlamba} i{ngane}, uku{sebenzisa} i{thoyilethi}." )
( data_0402 "{Bheka} ngoba baze bali{khipha} nge{sihloko} elithi le {ndawo} bazoyi{thatha} noma kanjani." )
( data_0403 "{Gcizelela} futhi ukuthi a{bantu} bange{sabi} uku{buza} i{mibuzo}." )
( data_0404 "Inani eli{phelele} le{ntela} ku{nyaka} o{gcwele}, u{suse} i{ntela} e{khokhelwa} a{basebenzi} e{khokhelwe} u{nyaka} o{gcwele}." )
( data_0405 "U{suse} u{mkhawulo} o{vunyelwe} wezi{mali} ze{ntela} yanga{phandle} yo{nyaka} o{gcwele}, ku{suswe} inani eli{khokhiwe} le{sikhathi} soku{qala} so{nyaka}." )
( data_0406 "{Qinisekisa} ukuthi une{gunya} lokw{enza} lokhu bese u{bhala} i{sikhundla} sakho nga{phakathi} e{nhlanganweni}." )
( data_0407 "U{Shaka} us{ebenzisa} u{mkhonto} o{phonswayo} njenge{sikhali} soku{gwaza} e{duze} kwe{sitha}, kunoku{ba} ame {kude} naso." )
( data_0408 "U{Ngqongqoshe} kanye no{mphathi}-{hhovisi} wakwa{hulumeni} o{gunyaziwe} ngu{Ngqongqoshe}." )
( data_0409 "Uma e{thola} i{sicelo} e{sibhalwe} {phansi} so{muntu} o{gunyaziwe} nga{phansi} kwe{sigatshana} se{sine}, angawa{qeda} a{mandla} alowo {muntu} oku{shadisa} ngoku{bhala} {phansi}." )
( data_0410 "Maku{be} {maqondana} nokuthi a{hlanganise} ubu{dlelwano} be{zakhamuzi} nga{phansi} kwalesi {Sahluko}." )
