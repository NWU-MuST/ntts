( data_0412 "Uzo{kwazi} uku{hlela} ka{ngcono}." )
( data_0413 "U{bambe} i{qhaza} ngoku{yimpumelelo} ezi{ngxoxweni} ezi{phathelene} ne{nqubomgomo} yo{mkhandlu} kanye nezi{nhlelo} noma i{misebenzi}." )
( data_0414 "Uzo{ba} {wazi} ka{banzi} ngo{mphakathi} wakho ukwe{dlula} {wonke} u{muntu}." )
( data_0415 "Selokhu u{qalile} lo {nyaka} i{sonto} li{phuma} li{gamenxe} i{hora} le{sibili} ku{ya} e{horeni} le{sithathu}." )
( data_0416 "{Gcwalisa} i{fomu} loku{faka} i{sicelo} e{hhovisi} laba{phethe} izi{moto} ezi{hamba} e{mgwaqweni} elise{duze} nawe." )
( data_0417 "Yayi{xoxwa} ngu{bani} pho ngoba waye{hamba} eyi{hlobisa} nga{mahlaya} ba{fe} yi{nsini}." )
( data_0418 "{Khumbuza} i{qembu} ukuthi i{sijobelelo} i{sithupha} singaba{nika} u{lwazi} olu{fingqiwe} ngo{khetho} nezi{nqubo} zoku{vota}." )
( data_0419 "U{Phumla} wa{thi} e{ngena} nje waye{tshela} u{mngani} wakhe ukuthi kune{ndaba} e{mnandi} azo{mxoxela} yona." )
( data_0420 "Wa{klewula} u{mkakhe} e{bona} i{ngane} ya{bantu} i{ndiza} e{moyeni} i{buya} i{thanqazeka} {phansi} e{tiyeleni}." )
( data_0421 "{Phuza} u{hlobo} olu{lodwa} lo{tshwala} nge{sikhathi} unga{phuzi} bese u{shayela} noma u{hambe} e{gwaqweni}." )
( data_0422 "Kwa{thi} lapho u{mama} e{qhaqha} i{sipho} engasi{phiwa} ku{baba} wakho ba{jabula} bonke e{ndlini}." )
( data_0423 "Ungaba{chaza} ka{njani} a{bantu} be{qembu} o{gxile} kulo ngokwalo {mklamo}." )
